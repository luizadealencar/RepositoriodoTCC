@startuml
entity Pessoa {
    -nome: String
}

entity Farmaceutico {
    -nome: String
}

entity Paciente {
    -cpf: String
    -numSUS: String
    -email: Date
    -dataNasc: Date
    -preferencial: boolean
}

entity HistoricoAtendimento {
    -receitamedica: BufferedImage
}

entity Remedio{
   -nomeRemedio: String
}

entity Atendimento {
    -dataHora: DateTime
}

entity Agenda {
    -dataHora: DateTime
    -limite: int
}

entity Notificacao {
    -mensagem: String
}

Pessoa ||--|{ Farmaceutico
Pessoa ||--|{ Paciente

Paciente }|..|| Atendimento : marca
Paciente }o..|| Notificacao : recebe

Farmaceutico }|--|{ Remedio : registra o medicamento
Farmaceutico }|..|| Atendimento : marca
Farmaceutico }|..|| HistoricoAtendimento : registra

Remedio }|--|{ HistoricoAtendimento : registrado no
Atendimento }|--|{ HistoricoAtendimento : registrado no

Agenda }o..|| Atendimento : utiliza a data e o hor√°rio disponibilizado
@enduml
